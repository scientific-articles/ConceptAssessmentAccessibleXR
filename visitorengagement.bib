    journal = {American Control Conference, 2005. Proceedings of the 2005},
    keywords = {acceleration, accelerometers, active, approximations, bio-sensors, biological, biosensors, body, cancellation, control, corruption, devices, distortion, estimation, finger, fir, hand, least, low-order, mems, method, micromechanical, model, motion, noise, photoplethysmograph, recursive, ring, sensor, signal, square, squares, techniques, wearable},
