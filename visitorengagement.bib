    journal = {Proc.\ Annual International Conference of the {IEEE} Engineering in Medicine and Biology Society. {IEEE} Engineering in Medicine and Biology Society},
    keywords = {acceleration, and, biomedical, computer-assisted, curve, electric, electrocardiography, electrodes, engineering, humans, impedance, motion, processing, roc, sensitivity, signal, specificity},
    pages = {1695--1698},
    posted-at = {2010-08-13 17:39:24},
